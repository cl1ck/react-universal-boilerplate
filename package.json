{
  "name": "rrr",
  "version": "0.0.1",
  "description": "React Redux RethinkDB",
  "main": "index.js",
  "author": "Michel Hofmann (michel.hofmann@bluewin.ch)",
  "repository": {
    "type": "git",
    "url": "https://github.com/cl1ck/rrr.git"
  },
  "scripts": {
    "start": "node_modules/path-exists-cli/cli.js \"./dist/server.js\" && pnpm run -s server || pnpm run -s build-then-start",
    "build-then-start": "pnpm run -s build && pnpm run -s server",
    "build": "pnpm run -s clean && cross-env NODE_ENV=production webpack --progress --colors",
    "build:watch": "cross-env NODE_ENV=production WATCH=true webpack --progress --colors --watch",
    "server": "node ./dist/server.js",
    "depcheck": "cross-env NODE_ENV=development babel-node src/dev/depcheck/depcheck.js",
    "format": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.jsx\"",
    "esdoc": "esdoc -c ./esdoc.json",
    "clean": "rimraf dist temp .nyc_output .tmp coverage apidoc logs",
    "dev": "pnpm run -s dev:prepare && pnpm run -s dev:server",
    "dev:prepare": "pnpm run -s clean && pnpm run -s db:up",
    "dev:server": "cross-env NODE_ENV=development babel-node ./src/dev/index.js",
    "db:up": "rethink-migrate up",
    "db:down": "rethink-migrate down",
    "ncu": "ncu",
    "nca": "ncu -a",
    "translate": "node ./src/dev/scripts/extractIntl.babel.js",
    "test": "npm-run-all test:unit test:acceptance",
    "test:ci": "npm-run-all --parallel test:unit test:acceptance:ci",
    "test:unit": "cross-env NODE_ENV=test webpack --colors --progress --config webpack.test.config.js",
    "test:unit:watch": "cross-env NODE_ENV=test webpack --colors --progress --watch --config webpack.test.config.js",
    "test:cover": "cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=text \"webpack --colors --progress --config webpack.test.config.js\"",
    "test:acceptance": "cross-env NODE_ENV=acceptance webpack --colors --progress --config webpack.test.config.js",
    "test:acceptance:watch": "cross-env NODE_ENV=acceptance webpack --colors --progress --watch --config webpack.test.config.js",
    "xvfb": "Xvfb -ac -screen scrn 1280x2000x24 :9.0 & export DISPLAY=:9.0"
  },
  "nyc": {
    "include": [
      "src/**/*.jsx?"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "license": "MIT",
